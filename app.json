{
  "expo": {
    "name": "Instamedirx",
    "slug": "InstaMediRx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mainlogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/mainlogo.ico",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vikiakan123.InstaMediRx"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/mainlogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "185aaee9-9921-4a71-afff-c5369ef9b181"
      }
    }
  }
}
